     laudos_avaliacoes_calor.`MEDIDAS_PROTECAO_INDIVIDUAL_OUTROS` AS laudos_avaliacoes_calor_MEDIDAS_PROTECAO_INDIVIDUAL_OUTROS,
     laudos_avaliacoes_calor.`ENQUADRAMENTO` AS laudos_avaliacoes_calor_ENQUADRAMENTO,
     laudos_avaliacoes_calor.`ACOES_PPRA` AS laudos_avaliacoes_calor_ACOES_PPRA,
     laudos_avaliacoes_calor.`FONTE_GERADORA` AS laudos_avaliacoes_calor_FONTE_GERADORA,
