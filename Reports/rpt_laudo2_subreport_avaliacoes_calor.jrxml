     estrutura_organizacional.`UNIDADE_PAI` AS estrutura_organizacional_UNIDADE_PAI,
     estrutura_organizacional.`NIVEL_ORGANIZACAO` AS estrutura_organizacional_NIVEL_ORGANIZACAO,
     estrutura_organizacional.`NIVEL_QUEBRA` AS estrutura_organizacional_NIVEL_QUEBRA,
     estrutura_organizacional.`DESCRICAO_UNIDADE` AS estrutura_organizacional_DESCRICAO_UNIDADE, maquinas.`NOME` as maquinas_NOME


FROM
     `laudos_avaliacoes_calor` laudos_avaliacoes_calor LEFT JOIN `laudos_avaliacoes_calor_funcoes` laudos_avaliacoes_calor_funcoes ON laudos_avaliacoes_calor_funcoes.`CODIGO_EMPRESA` = laudos_avaliacoes_calor.`CODIGO_EMPRESA`
     AND laudos_avaliacoes_calor_funcoes.`NUMERO_LAUDO` = laudos_avaliacoes_calor.`NUMERO_LAUDO`
     AND laudos_avaliacoes_calor_funcoes.`UNIDADE` = laudos_avaliacoes_calor.`UNIDADE`
     AND laudos_avaliacoes_calor_funcoes.`CODIGO_SETOR` = laudos_avaliacoes_calor.`CODIGO_SETOR`
     AND laudos_avaliacoes_calor_funcoes.`REVISAO` = laudos_avaliacoes_calor.`REVISAO`
     AND laudos_avaliacoes_calor_funcoes.`ANO_PERICIA` = laudos_avaliacoes_calor.`ANO_PERICIA`
     LEFT JOIN `laudos_avaliacoes_calor_maquinas` laudos_avaliacoes_calor_maquinas ON laudos_avaliacoes_calor.`CODIGO_EMPRESA` = laudos_avaliacoes_calor_maquinas.`CODIGO_EMPRESA`
     AND laudos_avaliacoes_calor_maquinas.`REVISAO` = laudos_avaliacoes_calor.`REVISAO`
     AND laudos_avaliacoes_calor_maquinas.`ANO_PERICIA` = laudos_avaliacoes_calor.`ANO_PERICIA`
     AND laudos_avaliacoes_calor_maquinas.`NUMERO_LAUDO` = laudos_avaliacoes_calor.`NUMERO_LAUDO`
     INNER JOIN `setores` setores ON laudos_avaliacoes_calor.`CODIGO_EMPRESA` = setores.`CODIGO_EMPRESA`
     AND setores.`UNIDADE` = laudos_avaliacoes_calor.`UNIDADE`
     AND setores.`CODIGO_SETOR` = laudos_avaliacoes_calor.`CODIGO_SETOR`
     AND setores.`CODIGO_EMPRESA` = laudos_avaliacoes_calor_maquinas.`CODIGO_EMPRESA`
     AND setores.`UNIDADE` = laudos_avaliacoes_calor_maquinas.`UNIDADE`
     AND setores.`CODIGO_SETOR` = laudos_avaliacoes_calor_maquinas.`CODIGO_SETOR`
     INNER JOIN `estrutura_organizacional` estrutura_organizacional ON setores.`CODIGO_EMPRESA` = estrutura_organizacional.`CODIGO_EMPRESA`
     AND estrutura_organizacional.`UNIDADE` = setores.`UNIDADE`
     INNER JOIN `maquinas` maquinas ON maquinas.`CODIGO_EMPRESA`=
laudos_avaliacoes_calor.`CODIGO_EMPRESA` AND maquinas.`CODIGO_MAQUINA` =
laudos_avaliacoes_calor_maquinas.`CODIGO_MAQUINA`
WHERE
     laudos_avaliacoes_calor.CODIGO_EMPRESA = $P{CODIGO_EMPRESA}
 AND laudos_avaliacoes_calor.ANO_PERICIA = $P{ANO_PERICIA}
 AND laudos_avaliacoes_calor.NUMERO_LAUDO = $P{NUMERO_LAUDO}
 AND laudos_avaliacoes_calor.REVISAO = $P{REVISAO}
ORDER BY
     laudos_avaliacoes_calor.UNIDADE ASC,
     laudos_avaliacoes_calor.CODIGO_SETOR ASC]]>
