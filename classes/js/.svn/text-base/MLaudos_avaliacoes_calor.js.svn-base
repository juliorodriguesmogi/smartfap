}
this.get_Acoes_ppra=function(nlinha){
	 return Acoes_ppra[nlinha];
}
this.setValores=function(response){
	 this.inicializa();
	 if (response[0]=='NF'){
		 for (x=0;x<response.length-1;x++){
		 vreg=response[x].split('|')
		Codigo_empresa[x]=vreg[0];
		Ano_pericia[x]=vreg[1];
		Numero_laudo[x]=vreg[2];
		Revisao[x]=vreg[3];
		Unidade[x]=vreg[4];
		Codigo_setor[x]=vreg[5];
		Tipo_avaliacao[x]=vreg[6];
		Sequencial[x]=vreg[7];
		Numero_funcs_setor[x]=vreg[8];
		Numero_funcs_expostos[x]=vreg[9];
		Periodicidade_exposicao[x]=vreg[10];
		Temperatura_nr17[x]=vreg[11];
		Temperatura_medida[x]=vreg[12];
		Medidas_protecao_coletivas[x]=vreg[13];
		Medidas_protecao_individual[x]=vreg[14];
		Enquadramento[x]=vreg[15];
		Id_usuario[x]=vreg[16];
		Session_id[x]=vreg[17];
		Ultima_atualizacao[x]=vreg[18];
		Ultimo_usuario[x]=vreg[19];
		Observacoes[x]=vreg[20];
		Acoes_ppra[x]=vreg[21];
		 }
	}
}
};
newlaudosavaliacoescalor=new MLaudos_avaliacoes_calor();
